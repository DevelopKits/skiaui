<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <OutDir>$(SampleOutputPath)\$(Configuration)\</OutDir>
    <IntDir>$(Configuration)\$(ProjectName)\</IntDir>
    <IncludePath>$(SkiauiProjectPath);$(IncludePath)</IncludePath>
    <LibraryPath>$(LibOutputPath)\$(Configuration);$(LibraryPath)</LibraryPath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(SkiaPath)\include\config;$(SkiaPath)\include\core;$(SkiaPath)\include\views;$(SkiaPath)\include\xml;$(SkiaPath)\include\utils;$(SkiaPath)\include\images;$(SkiaPath)\include\effects;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
    <Link>
      <AdditionalDependencies>skia_core.lib;skia_views.lib;skia_utils.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>